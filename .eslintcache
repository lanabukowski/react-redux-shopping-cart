';\n\nconst ProductItem = ( {menuItem, onAddToCart}) => {\n    const {name, prices, picture} = menuItem;\n    return (\n        <>\n            <div className='productItemWrapper'>\n                <div className='productItemBlock'>\n                    <div className='productItemImg'><img width=\"150\" height=\"150\" src={picture}></img></div>\n                    <div className='productItemTitle'>{name}</div>\n                    <div className='price'>{prices} $</div>\n                    <button onClick={() => onAddToCart()} className=\"button button-bag\">Add to Bag</button>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default ProductItem;\n","/Users/lana/Desktop/проэкты/react/shop-react-redux-app/src/components/loading/loading.js",[],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":1,"column":8,"nodeType":"106","messageId":"107","endLine":1,"endColumn":13},{"ruleId":"104","severity":1,"message":"108","line":63,"column":19,"nodeType":"106","messageId":"107","endLine":63,"endColumn":27},{"ruleId":"109","severity":1,"message":"110","line":68,"column":64,"nodeType":"111","messageId":"112","endLine":68,"endColumn":66},{"ruleId":"113","severity":1,"message":"114","line":13,"column":60,"nodeType":"115","endLine":13,"endColumn":115},{"ruleId":"104","severity":1,"message":"116","line":1,"column":8,"nodeType":"106","messageId":"107","endLine":1,"endColumn":13},{"ruleId":"104","severity":1,"message":"117","line":3,"column":10,"nodeType":"106","messageId":"107","endLine":3,"endColumn":14},{"ruleId":"118","severity":1,"message":"119","line":9,"column":25,"nodeType":"120","messageId":"121","endLine":9,"endColumn":27},{"ruleId":"113","severity":1,"message":"114","line":20,"column":58,"nodeType":"115","endLine":20,"endColumn":102},{"ruleId":"113","severity":1,"message":"114","line":10,"column":53,"nodeType":"115","endLine":10,"endColumn":97},"no-native-reassign",["122"],